<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <fieldset name="settings">
        <field name="extension_attributes.shipping_type" sortOrder="500" formElement="select">
            <argument name="data" xsi:type="array">
                <item name="config" xsi:type="array">
                    <item name="source" xsi:type="string">extension_attributes.shipping_type</item>
                </item>
            </argument>
            <settings>
                <dataType>text</dataType>
                <label translate="true">Shipping type</label>
                <visible>true</visible>
            </settings>
            <formElements>
                <select>
                    <settings>
                        <options class="Wagento\OfflineShipping\Model\Company\Source\ShippingType"/>
                    </settings>
                </select>
            </formElements>
        </field>
    </fieldset>
</form>
